{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": ">=8.3.0",
		"yarn": ">=1.2"
	},
	"dependencies": {
		"axios": "^0.17.0",
		"classnames": "^2.2.5",
		"css-loader": "^0.28.4",
		"geocoder": "^0.2.3",
		"lodash": "latest",
		"node-sass-chokidar": "0.0.3",
		"npm-run-all": "^4.1.2",
		"prop-types": "^15.6.0",
		"react": "^16.1.0",
		"react-async-script-loader": "^0.3.0",
		"react-dom": "^16.1.0",
		"react-google-button": "^0.4.0",
		"react-google-maps": "^9.2.2",
		"react-modal": "^3.1.2",
		"react-places-autocomplete": "^5.3.1",
		"react-redux": "^5.0.5",
		"react-router": "^3.2.0",
		"react-router-bootstrap": "^0.24.2",
		"react-router-dom": "^4.1.2",
		"react-router-redux": "^4.0.8",
		"react-scripts": "1.0.17",
		"react-social-login": "^3.4.1",
		"redux": "^3.7.1",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.2.0",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.19.0"
	},
	"devDependencies": {},
	"scripts": {
		"build-css":
			"node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
		"watch-css":
			"npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
		"start-js": "react-scripts start",
		"start": "npm-run-all -p watch-css start-js",
		"build": "npm run build-css && react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject"
	},
	"proxy": {
		"/auth/google": {
			"target": "http://localhost:5000"
		},
		"/api/*": {
			"target": "http://localhost:5000"
		}
	}
}
